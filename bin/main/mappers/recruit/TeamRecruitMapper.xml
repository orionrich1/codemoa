<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.codemoa.project.domain.chat.mapper.ChatMapper">

<select id="getTeamRecruitList" resultType="TeamRecruitList">
SELECT *
FROM recruitbbs;
WHERE no = #{recruitId}
</select>

<select id="TeamRecruitList" resultType="TeamRecruit">
SELECT *
FROM recruitbbs;
ORDER BY no DESC
</select>
</mapper>